{
  "manifest_version": 2,

  "name": "Goto GoDoc",
  "description": "Open the currently selected page in pkg.go.dev",
  "version": "0.6.1",

  "permissions": [ "tabs" ],

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "browser_action": {
    "default_icon": "icon128.png",
    "default_title": "Goto GoDoc"
  },

  "background": {
     "scripts": [ "background.js" ]
  }
}

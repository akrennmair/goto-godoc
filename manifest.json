{
  "manifest_version": 3,

  "name": "Goto GoDoc",
  "description": "Open the currently selected page in pkg.go.dev",
  "version": "0.8",

  "permissions": [ "tabs" ],

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "action": {
    "default_icon": "icon128.png",
    "default_title": "Goto GoDoc"
  },

  "background": {
     "service_worker": "background.js",
     "type": "module"
  }
}
